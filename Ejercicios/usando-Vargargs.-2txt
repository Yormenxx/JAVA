import java.util.*;

public class Main {
    public static void main(String[] args) throws Exception {
        // Manejando vararg
        
        PersonaNumeros("Juabn", 20 , 2,3,4,5,65);
        
        
    }
    
    private static void PersonaNumeros(String nombre, int Edad, int... numeros){
        
        System.out.println("Persona y numeros" + nombre + " " + Edad );
        ImprimirNumeros(numeros);
        
    };

       
    private static void ImprimirNumeros(int... numeros){
        
        for (int i = 0 ; i< numeros.length; i++ ){
            
            System.out.println("numeros : " + numeros[i]);
            
        };
        
    }
    

}

